{
  "name": "express-app-template",
  "version": "1.0.0",
  "description": "Template for Express Application with TypeScript",
  "author": "S Abd Malik",
  "license": "MIT",
  "main": "dist/bin/www",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "start": "./node .",
    "start:prod": "cross-env NODE_ENV=production npm start",
    "start:pm2": "pm2-runtime ecosystem.config.js",
    "start:dev": "cross-env NODE_ENV=development env-cmd tsnd -r tsconfig-paths/register --rs --clear --respawn --transpile-only src/bin/www",
    "start:debug": "cross-env NODE_ENV=development DEBUG=app:* env-cmd tsnd -r tsconfig-paths/register --rs --clear --respawn --transpile-only --debug --inspect=0.0.0.0:9229 -- src/bin/www",
    "test": "env-cmd -f .env.test.local jest --config ./jest-e2e.config.ts --runInBand",
    "test:cov": "npm test -- --coverage",
    "test:before": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "test:after": "sequelize db:drop",
    "format": "prettier -w \"{config,db,public,src}/**/*.{md,html,css,json,js,ts}\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "seq": "env-cmd sequelize",
    "prepare": "husky"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.16.4",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.15.0",
    "helmet": "^5.0.2",
    "http-errors": "^1.6.3",
    "husky": "^9.0.11",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^6.8.0",
    "mongoose": "^8.6.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pug": "^3.0.2",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/eslint": "^8",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.11.14",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.11.8",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5 && <5.6.0"
  },
  "keywords": [
    "node",
    "nodejs",
    "express",
    "expressjs",
    "sql",
    "postgresql",
    "sequelize",
    "sequelize-cli",
    "crud",
    "restapi",
    "jwt",
    "blog"
  ],
  "repository": "https://github.com/ssamsara98/express-app-template.git",
  "bugs": "https://github.com/ssamsara98/express-app-template/issues",
  "homepage": "https://github.com/ssamsara98/express-app-template#readme",
  "lint-staged": {
    "*.{md,html,css,json}": "prettier -w",
    "*.{js,ts}": "eslint --cache --fix"
  }
}
