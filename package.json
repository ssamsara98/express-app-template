{
  "name": "express-app-template",
  "version": "1.0.0",
  "description": "Template for Express Application with TypeScript",
  "author": "S Abd Malik",
  "license": "MIT",
  "main": "dist/bin/www",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -b",
    "start": "node -r tsconfig-paths/register .",
    "start:prod": "cross-env NODE_ENV=production npm start",
    "start:dev": "cross-env NODE_ENV=development env-cmd tsnd -r tsconfig-paths/register --rs --clear --respawn --transpile-only src/bin/www",
    "start:debug": "cross-env NODE_ENV=development DEBUG=app:* env-cmd tsnd -r tsconfig-paths/register --rs --clear --respawn --transpile-only --debug --inspect=0.0.0.0:9229 -- src/bin/www",
    "format": "prettier -w \"{config,db,public,src}/**/*.{md,html,css,json,js,ts}\"",
    "seq": "env-cmd sequelize",
    "prepare": "husky install"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.16.4",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.15.0",
    "helmet": "^5.0.2",
    "http-errors": "^1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pug": "^3.0.2",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^1.8.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.11.14",
    "@types/validator": "^13.11.8",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "prettier": "^3.2.4",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "node",
    "nodejs",
    "express",
    "expressjs",
    "sql",
    "postgresql",
    "sequelize",
    "sequelize-cli",
    "crud",
    "restapi",
    "jwt",
    "blog"
  ],
  "repository": "https://github.com/ssamsara98/express-app-template.git",
  "bugs": "https://github.com/ssamsara98/express-app-template/issues",
  "homepage": "https://github.com/ssamsara98/express-app-template#readme",
  "lint-staged": {
    "*.{md,html,css,json,js,ts}": "prettier -w"
  }
}
